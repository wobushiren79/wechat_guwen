.citypickers {
  position: fixed;
  height: 100%;
  width: 100%;
  min-height: 100%;
  background-color: red;
}

.citypicker {
}

.citybody {
  position: fixed;
  bottom: 0px;
}

.cityheader {
  position: absolute;
  top: 0px;
  width: 100%;
  z-index: 4;
}

.city-cancel {
  float: left;
  margin: 20rpx;
  color: #818181;
}

.city-true {
  float: right;
  margin: 20rpx;
  color: #2fb42e;
}

.section .picker {
  background-color: #fff;
  border-bottom: 1px #d9d9d9 solid;
  border-top: 1px #d9d9d9 solid;
  padding: 20rpx;
}

/* 省市区下拉 end */

/* 单项服务订单列表 star */

.page_service_order .tab view {
  width: 20%;
}

.page_service_order .icon {
  width: 14rpx;
  height: 24rpx;
  position: absolute;
  top: 50%;
  right: 16rpx;
  transform: translateY(-50%);
  opacity: 0.5;
  transition: all 0.3s;
}

.page_service_order .list .ul {
  padding: 0 16rpx;
}

.page_service_order .list .ul .li {
  margin-top: 15rpx;
  background: #fff;
  font-size: 28rpx;
  position: relative;
  z-index: 1;
  border-radius: 5rpx;
}

.page_service_order .list .ul .li .li_header,
.page_service_order .list .ul .li .li_footer {
  display: flex;
  padding: 0 16rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #efefef;
  color: #48cfad;
  height: 80rpx;
}

.page_service_order .list .ul .li .li_footer {
  border: none;
}

.page_service_order .list .ul .li .li_header .header,
.page_service_order .list .ul .li .li_footer .header {
  flex-basis: 180rpx;
}

.page_service_order .list .ul .li .li_header .footer,
.page_service_order .list .ul .li .li_footer .footer {
  flex: 1;
  text-align: right;
}
.page_service_order .list .ul .li .li_header .footer navigator image{
  /* display: block */
  width: 60rpx;
  position: relative;
  top: 10rpx;
}
.page_service_order .list .ul .li .li_footer .footer {
  position: relative;
  z-index: 99;
}

.page_service_order .list .ul .li .li_footer image {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  margin-right: 4rpx;
  position: relative;
  top: 15rpx;
}

.page_service_order .list .ul .li .li_footer .footer .btn,
.page_allot_order_list .list .ul .li .li_header .footer .btn,
.page_allot_order_list .list .ul .li .li_footer .header .btn {
  display: inline-block;
  padding: 0;
  margin: 0;
  margin-left: 16rpx;
  width: 132rpx;
  height: 50rpx;
  text-align: center;
  border: 1px solid #48cfad;
  border-radius: 5rpx;
  color: #fff;
  box-sizing: border-box;
  background: #48cfad;
  outline: none;
  line-height: 46rpx;
  font-size: 28rpx;
}

.page_service_order .list .ul .li .li_footer .footer .btn.btn_1,
.page_allot_order_list .list .ul .li .li_header .footer .btn.btn_1,
.page_allot_order_list .list .ul .li .li_footer .header .btn.btn_1 {
  background: transparent;
  color: #48cfad;
}

.page_service_order .list .ul .li .li_footer .footer .btn::after,
.page_allot_order_list .list .ul .li .li_header .footer .btn::after,
.page_allot_order_list .list .ul .li .li_footer .header .btn::after {
  display: none;
}

.page_service_order .list .ul .li .li_footer .footer .btn:active,
.page_service_order .list .ul .li .li_footer .header:active,
.page_allot_order_list .list .ul .li .li_header .footer .btn:active,
.page_allot_order_list .list .ul .li .li_footer .header .btn:active {
  opacity: 0.7;
}

.page_service_order .list .ul .li .li_body {
  border-bottom: 1px solid #efefef;
  padding: 16rpx;
  box-sizing: border-box;
  position: relative;
  z-index: 2;
}

.page_service_order .list .ul .li .li_body>view {
  display: flex;
}

.page_service_order .list .ul .li .li_body>view .header {
  flex-basis: 150rpx;
}

.page_service_order .list .ul .li .li_body>view .body {
  flex: 1;
}

/* 单项服务订单列表 end */

/* 分单系统订单列表 star */

.page_allot_order_list .tab view {
  width: 25%;
}

.page_allot_order_list .list .ul .li .li_header .header,
.page_allot_order_list .list .ul .li .li_footer .header {
  flex-basis: 550rpx;
}

.page_allot_order_list .list .ul .li .li_footer .header>view {
  float: left;
  margin-right: 10rpx;
}

.page_allot_order_list .list .ul .li .li_footer .header:active {
  opacity: 1;
}

.page_allot_order_list .list .ul .li .li_footer .header>view:active {
  opacity: 0.7;
}

.page_allot_order_list .list .ul .li .li_footer .header .btn {
  margin-left: 0;
  margin-right: 16rpx;
}

.page_allot_order_list .list .ul .li .li_footer .footer {
  flex: 1;
  text-align: right;
  /* background: #353535; */
}

.page_allot_order_list .list .ul .li .li_header .footer,
.page_allot_order_list .list .ul .li .li_footer .footer{
  position: relative;
}
.page_allot_order_list .list .ul .li .li_header .footer .btn,
.page_allot_order_list .list .ul .li .li_footer .footer .btn  {
  position: absolute;
  top: 50%;
   right: 0; 
  transform: translate(0, -50%);
}

/* 分单系统订单列表 end */

/* 尾部警告 */
.warning_text{
  text-align: center;
