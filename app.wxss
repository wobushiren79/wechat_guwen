@import 'style/weui.wxss';

page {
  background-color: #f8f8f8;
  font-size: 16px;
  font-family: "Microsoft Yahei";
}

.page__hd {
  padding: 40px;
}

.page__bd {
  padding-bottom: 40px;
}

.page__bd_spacing {
  padding-left: 15px;
  padding-right: 15px;
}

.page__ft {
  padding-bottom: 10px;
  text-align: center;
}

.page__title {
  text-align: center;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888;
  text-align: center;
  font-size: 14px;
}

.weui-cell_input .icon image {
  width: 23px;
  height: 23px;
  display: block;
}

.weui-cell_input .icon {
  position: absolute;
  top: 12px;
  right: 12px;
  z-index: 99;
}

.weui-cell_input_icon {
  padding-right: 40px;
}

.button-hover[type="primary"] {
  background-color: #3eac90;
}

button[type="primary"] {
  color: #fff;
  background-color: #48cfad;
}

/*tab*/

.tab {
  text-align: center;
  /* padding: 20rpx 0; *//* border-bottom: 1px solid #3eac90; */
  height: 115rpx;
  background-color: #fff;
  box-sizing: border-box;
}

.tab view {
  display: block;
  float: left;
  width: 25%;
  line-height: 111rpx;
  font-size: 28rpx;
  color: #a9a9a9;
}

.tab view navigator {
  display: inline-block;
}

.tab navigator.active {
  color: #48cfad;
  border-bottom: 2px solid #48cfad;
}

.wxParse-img {
  /*background-color: #efefef;*/
  overflow: hidden;
  /*dqr_20170617*/
  margin-left: 0em;
}

/*清楚浮动*/

.clearfix::after {
  content: "";
  display: block;
  clear: both;
  height: 0;
  font-size: 0;
}

.clearfix {
  zoom: 1;
}

input {
  font-family: "Microsoft Yahei" !important;
}

.aui-ellipsis-1 {
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  word-break: break-all;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.aui-ellipsis-2 {
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  word-break: break-all;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.mask {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 8;
}

/* 置底页脚 */

.footerfix button {
  border-radius: 0;
  outline: none;
  border: none;
}

