  padding-bottom: 20rpx;
}

/* 新建 star */

.fix_new {
  background: #fff;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  position: fixed;
  bottom: 50rpx;
  right: 50rpx;
  z-index: 99;
  color: #fff;
  text-align: center;
  line-height: 72rpx;
  font-size: 60rpx;
  background-color: rgba(72, 207, 173, 0.8);
  box-shadow: 0px 0px 5px #fff;
}
.fix_new text{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%)
}

