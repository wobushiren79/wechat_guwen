<view class="page" xmlns:wx="http://www.w3.org/1999/xhtml">
  <view class="page__bd page_service_order page_allot_order_list page_allot_order_list2">
    <!--选项卡-->
    <view class="tab">
      <view>
        <navigator url="/pages/order/CashBack/CashBack_list_wait/CashBack_list_wait" open-type="redirect">服务中</navigator>
      </view>
      <view>
        <navigator url="/pages/order/CashBack/CashBack_list_processing/CashBack_list_processing"  open-type="redirect">未返现</navigator>
      </view>
      <view>
        <navigator url="/pages/order/CashBack/CashBack_list_audit/CashBack_list_audit" class="active"  open-type="redirect">已返现</navigator>
      </view>
      <view>
        <navigator url="/pages/order/CashBack/CashBack_list_finish/CashBack_list_finish" open-type="redirect" >已关闭</navigator>
      </view>
    </view>

    <!-- 列表 -->
    <view class="list">
      <view class="ul">


        <!-- 模板1 待接单 star-->
        <block wx:for="{{listdata}}">

          <block wx:if="{{item.workOrder.orderType==0}}">
            <view class="li plan">
              <view class="li_header plan">
                <view class="header">
                  <image src='/images/icon_order_list_plan.png' mode='widthFix'></image>
                </view>
                <view class='body'>
                  殡仪/{{item.workOrder.orderNumber}}
                </view>
                <view class='footer'>
                  ¥{{item.workOrderUserFinances[0].commissionPrice?item.workOrderUserFinances[0].commissionPrice/100:'-'}}
                </view>
              </view>
              <navigator class="li_body" url="/pages/CashBack/CashBack_content/CashBack_content?orderId={{item.workOrder.id}}" hover-class="active">
                <image src="/images/right.png" class="icon"></image>
                <view>
                  <view class="header">
                    服务地址：
                  </view>
                  <view class="body">
                    {{item.customerInfo.address}}
                  </view>
                </view>
                <view>
                  <view class="header">
                    预约时间：
                  </view>
                  <view class="body">
                    {{item.workOrder.appointmentTime}}
                  </view>
                </view>
                <view>
                  <view class="header">
                    客户姓名：
                  </view>
                  <view class="body">
                    {{item.customerInfo.contactName}}
                  </view>
                </view>
                <view>
                  <view class="header">
                    客户电话：
                  </view>
                  <view class="body">
                    {{item.customerInfo.contactPhone}}
                  </view>
                </view>
                <block wx:if="{{item.auditorName.length>0}}">
                  <view>
                    <view class="header">
                      审核员:
                    </view>
                    <view class="body">
                      {{item.auditorName}}
                    </view>
                  </view>
                </block>
              </navigator>
            </view>
          </block>
          <block wx:elif="{{item.workOrder.orderType==1}}">
            <view class="li cem">
              <view class="li_header cem">
                <view class="header">
                  <image src='/images/icon_order_list_cem.png' mode='widthFix'></image>
                </view>
                <view class='body'>
                  公墓/{{item.workOrder.orderNumber}}
                </view>
                <view class='footer'>
                  {{item.workOrder.orderStatusStr}}
                </view>
              </view>
              <navigator class="li_body" url="/pages/CashBack/CashBack_content/CashBack_content?orderId={{item.workOrder.id}}" hover-class="active">
                <image src="/images/right.png" class="icon"></image>
                <view>
                  <view class="header">
                    服务地址：
                  </view>
                  <view class="body">
                    {{item.customerInfo.address}}
                  </view>
                </view>
                <view>
                  <view class="header">
                    预约时间：
                  </view>
                  <view class="body">
                    {{item.workOrder.appointmentTime}}
                  </view>
                </view>
                <view>
                  <view class="header">
                    客户姓名：
                  </view>
                  <view class="body">
                    {{item.customerInfo.contactName}}
                  </view>
                </view>
                <view>
                  <view class="header">
                    客户电话：
                  </view>
                  <view class="body">
                    {{item.customerInfo.contactPhone}}
                  </view>
                </view>
                <block wx:if="{{item.auditorName.length>0}}">
                  <view>
                    <view class="header">
                      审核员:
                    </view>
                    <view class="body">
                      {{item.auditorName}}
                    </view>
                  </view>
                </block>
              </navigator>
            </view>
          </block>
          <block wx:elif="{{item.workOrder.orderType==2||item.workOrder.orderType==null}}">
            <view class="li other">
              <view class="li_header other">
                <view class="header">
                  <image src='/images/icon_order_list_other.png' mode='widthFix'></image>
                </view>
                <view class='body'>
                  其它/{{item.workOrder.orderNumber}}
                </view>
                <view class='footer'>
                  {{item.workOrder.orderStatusStr}}
                </view>
              </view>
              <navigator class="li_body" url="/pages/CashBack/CashBack_content/CashBack_content?orderId={{item.workOrder.id}}" hover-class="active">
                <image src="/images/right.png" class="icon"></image>
                <view>
                  <view class="header">
                    服务地址：
                  </view>
                  <view class="body">
                    {{item.customerInfo.address}}
                  </view>
                </view>
                <view>
                  <view class="header">
                    预约时间：
                  </view>
                  <view class="body">
                    {{item.workOrder.appointmentTime}}
                  </view>
                </view>
                <view>
                  <view class="header">
                    客户姓名：
                  </view>
                  <view class="body">
                    {{item.customerInfo.contactName}}
                  </view>
                </view>
                <view>
                  <view class="header">
                    客户电话：
                  </view>
                  <view class="body">
                    {{item.customerInfo.contactPhone}}
                  </view>
                </view>
                <block wx:if="{{item.auditorName.length>0}}">
                  <view>
                    <view class="header">
                      审核员:
                    </view>
                    <view class="body">
                      {{item.auditorName}}
                    </view>
                  </view>
                </block>
              </navigator>
            </view>
          </block>
        </block>
      </view>
    </view>

  </view>
  <import src="/template/template.wxml"/>
  <block wx:if="{{notNumber}}">
  <block wx:if="{{listdata.length>0}}" >
  <template is="nonconformance"/>
  </block>
    <block wx:else>
    <template is="nullData"/>
    </block>
  </block>


</view>