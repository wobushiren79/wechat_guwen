
.content{
  text-align: center;
  padding-top: 60rpx;
}
.content .title{
  font-size: 36rpx;
  color: #353535;
  font-weight: 600
}
.content .text{
  font-size: 24rpx;
  color: #727272;
  margin: 60rpx 0 140rpx;
}
.content .text text{
  color: #48cfad
}
.content image{
  margin: auto;
  width: 120rpx;
  height: 120rpx;
}
.footerfix {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  text-align: center;
  background: #fff;
  border-top: 1px solid #efefef;
}


.footerfix .keep {
  height: 100rpx;
  color: #ffffff;
  font-size: 32rpx;
  line-height: 100rpx;
  background: linear-gradient(to left, #3eb99a, #269080);
}